{
    "version": 2,
    "builds": [
      { "src": "src/index.js", "use": "@vercel/node" },
      { "src": "backend/index.js", "use": "@vercel/node" }
    ],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/index.js" },
      { "src": "/(.*)", "dest": "/src/index.js" }
    ]
  }
  